/**app.wxss**/

.select-des {
  font-size: 14px;
  color: #666;
  padding: 20px;
}
.applet-collapse-item {
  padding: 0 15px !important;
  margin: -1px 0;
}
.applet-cell {
  justify-content: space-between;
}
.applet-cell > .van-cell__title,
.applet-cell > .van-cell__value {
  flex: unset;
}

.result-wrap {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  opacity: .8;
}

