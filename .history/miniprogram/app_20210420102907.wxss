/**app.wxss**/

.select-des {
  font-size: 14px;
  color: #666;
  padding: 20px;
}
.applet-collapse-item {
  padding: 0 15px !important;
  margin: -1px 0;
}
.applet-cell {
  justify-content: space-between;
}
.applet-cell > .van-cell__title,
.applet-cell > .van-cell__value {
  flex: unset;
}

.result-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #ffffff;
  opacity: .8;
  display: none;
}

.result-wrap.show {
  display: block;
}

.result-wrap .close {
  position: absolute;
  top: 30px;
  right: 30px;
  display: flex;
}

.result-wrap .main {
  justify-content: center;
  align-items: center;
  width: 260px;
  height: 260px;
  border-radius: 50%;
  overflow: hidden;
}

